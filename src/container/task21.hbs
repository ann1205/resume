<main>
    {{#with heading}}
    <div class="card">
        <div class="card-body">
            {{#with main}}
            <div class="card">
                <div class="card-body">
                    <h1>{{title}}</h1>
                    <p>{{text}}</p>
                    {{#with button_text}}
                    <button class="btn btn-primary">{{solid}}</button>
                    <br>
                    <br>
                    <button class="btn btn-outline-primary">{{outline}}</button>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>        
        <div class="card-body">
            {{#with second}}
            <div class="card">
                <div class="card-body">
                    <h2>{{title}}</h2>
                    <p>{{text}}</p>
                    {{#with button_text}}
                    <button class="btn btn-secondary">{{solid}}</button>
                    <br>
                    <br>
                    <button class="btn btn-outline-secondary">{{outline}}</button>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
        <div class="card-body">
            {{#with subblock}}
            <div class="card">
                <div class="card-body">
                    <h3>{{title}}</h3>
                    <p>{{text}}</p>
                    {{#with button_text}}
                    <button class="btn btn-dark">{{solid}}</button>
                    <br>
                    <br>
                    <button class="btn btn-outline-dark">{{outline}}</button>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>        
    {{/with}}
    <br>
    <br>
    {{#with nested_display}}
        <div class="card">
            <div class="display-1"> {{title}}</div>
            <div class="card-body">
                {{#with children_display}}
                <div class="card">
                    <div class="card-body">
                        <div class="display-4"> {{title}}</div>
                        <p>{{text}}</p>
                        {{#with button}}
                        <button class="btn btn-outline-warning">{{text}}</button>
                        {{/with}}
                    </div>
                </div>
                {{/with}}
                <br>                
                <h6>{{header_text}}</h6>
                
                <br>
                {{#with button}}
                    <button class="btn btn-info">{{text}}</button>
                {{/with}}
            </div>
        </div>
    {{/with}}
    <br>
    <br>
    {{#with paragraph}}
        <div class="card">
            <div class="card-body">
                <div class="text-uppercase">{{title}}</div>
                <br>                
                <div class="text-lowercase">{{text}}</div>
                <br>
                {{#with button}}
                    <button class="btn btn-link" href="{{url}}">{{text}}</button>
                {{/with}}                
                <div class="card">
                    <div class="card-body">
                        <ul>
                            {{#each list_name}}
                            <li>
                                <button class="btn btn-link" href="{{url}}">{{text}}</button>
                            </li>
                            {{/each}}
                        </ul>
                    </div>
                </div>                
            </div>
        </div>
    {{/with}}
</main>
{{#with service}}
        <div class="alert text-bg-success" role="alert"> 
            <h2 class="text-center">{{title}}</h2>
            <p class="text-center">{{description}}</p>
            {{#with buttons}}
            <div class="text-center">
                <button class="btn btn-primary" href="{{[0].link}}">{{[0].text}}</button>
            </div>
            {{/with}}            
        </div>
        {{/with}}